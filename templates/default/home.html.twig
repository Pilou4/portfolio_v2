        <h2 class="home__subtitle" id="contact">Me contacter</h2>
        <section class="home__contact reveal-">
            <p class="home__contact__error">
                {% for label, messages in app.flashes %}
                    {% for message in messages %}
                        <div id="alert" class="alert alert-{{ label }}">
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
                
            </p>
            <div class="home__contact__form">
                {{ form_start(form) }}
                {{ form_row(form.object) }}
                <div class="home__contact__form__name">
                    {{ form_row(form.firstname) }}
                    {{ form_row(form.lastname) }}
                </div>
                {{ form_row(form.email) }}
                {{ form_row(form.message) }}
                <div class="home__contact__form__authorize">
                    {{ form_row(form.agreeTerms, { 'attr': {'class': 'home__contact__form__authorize'} }) }}
                    <p class="home__contact__form__authorize__text">
                        Pour connaître et exercer vos droits, veuillez consultez la
                        <a href="{{ path('confidentiality') }}" class="home__contact__form__authorize__link">Politique de confidentialité.</a>
                    </p>
                </div>
                <button class="home__contact__form__button">Envoyer</button>
