{% extends 'base.html.twig' %}

    {% block url %}
        <link rel="canonical" href="https://frederic-caffier.fr/" />
    {% endblock %}

    {% block metaTag %}
        <meta name="description" content="portfolio de Frédéric CAFFIER développeur web junior">
